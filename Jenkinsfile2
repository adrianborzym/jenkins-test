pipeline{
    agent any
    stages{
        stage('Build') {
            parallel {
                stage('Build1'){
                    echo "make build1"
                }
                stage('Build2'){
                    echo "make Build2"
                }
                stage('Build3'){
                    echo "make Build3"
                }
            }
        }
    }
    post{
        always{
            echo "========always======== file test"
        }

    }
}
